# Default provider
VITE_LLM_PROVIDER=mock
# Use dev proxy to avoid CORS (recommended for local testing)
VITE_LLM_USE_PROXY=false

# OpenAI
VITE_OPENAI_API_KEY=
VITE_OPENAI_MODEL=gpt-4o-mini
VITE_OPENAI_BASE_URL=https://api.openai.com/v1

# Gemini
VITE_GEMINI_API_KEY=
VITE_GEMINI_MODEL=gemini-1.5-pro
VITE_GEMINI_BASE_URL=https://generativelanguage.googleapis.com

# DeepSeek
VITE_DEEPSEEK_API_KEY=
VITE_DEEPSEEK_MODEL=deepseek-chat
